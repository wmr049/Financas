@model IList<Financas.Entidades.Movimentacao>

@Html.ActionLink("Nova Movimentação", "form", "Movimentacao")

<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Usuario</th>
            <th>Valor</th>
            <th>Tipo</th>
        </tr>
    </thead>
    <tbody>
        <tr>
    @foreach (var m in Model)
    {
        <tr>
            <td>@m.Id</td>
            <td>@m.Usuario.Nome</td>
            <td>@m.Valor</td>
            <td>@m.tipo</td>
        </tr>
    }
        
    </tbody>
</table> 